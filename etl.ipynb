   "source": [
    "# Anny - Spells used by characters in the first 3 movies."
   ],
   "metadata": {}
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "source": [
    "spell_list = \"Sources\\Spells.csv\"\r\n",
    "movie_1 = \"Sources\\Harry Potter 1.csv\"\r\n",
    "movie_2 = \"Sources\\Harry Potter 2.csv\"\r\n",
    "movie_3 = \"Sources\\Harry Potter 3.csv\"\r\n",
    "\r\n",
    "spells_df = pd.DataFrame(pd.read_csv(spell_list,sep=\";\"))\r\n",
    "script_1_df = pd.DataFrame(pd.read_csv(movie_1,sep=\";\"))\r\n",
    "script_2_df = pd.DataFrame(pd.read_csv(movie_2,sep=\";\"))\r\n",
    "script_3_df = pd.DataFrame(pd.read_csv(movie_3,sep=\";\")).rename(columns=str.title)"
   ],
   "outputs": [],
   "metadata": {}
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "source": [
    "script_full = pd.concat([script_1_df,script_2_df,script_3_df])"
   ],
   "outputs": [],
   "metadata": {}
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "source": [
    "spell_names_df = spells_df[spells_df[\"Incantation\"] != \"Unknown\"].dropna()\r\n",
    "spell_names_df = spell_names_df[spell_names_df[\"Incantation\"] != \"None\"]"
   ],
   "outputs": [],
   "metadata": {}
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "source": [
    "spell_names = spell_names_df[\"Incantation\"].tolist()"
   ],
   "outputs": [],
   "metadata": {}
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "source": [
    "name = []\r\n",
    "says = []\r\n",
    "spell = []\r\n",
    "\r\n",
    "for index, row in script_full.iterrows():\r\n",
    "    for spell_name in spell_names:\r\n",
    "        if spell_name in row[\"Sentence\"]:\r\n",
    "            name.append(row[\"Character\"])\r\n",
    "            says.append(row[\"Sentence\"])\r\n",
    "            spell.append(spell_name)\r\n",
    "            break"
   ],
   "outputs": [],
   "metadata": {}
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "source": [
    "filtered_dict = list(zip(name,says,spell))\r\n",
    "filtered_df = pd.DataFrame(filtered_dict)"
   ],
   "outputs": [],
   "metadata": {}
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "source": [
    "final_df = filtered_df.rename(columns={0:\"Name\",1:\"Sentence\",2:\"Spell\"})\r\n",
    "final_df[\"Name\"] = final_df[\"Name\"].str.title()"
   ],
   "outputs": [],
   "metadata": {}
  },
